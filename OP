<rolltemplate class="sheet-rolltemplate-SSkillCheck">
    <div class="container">
        <div><h1>{{name}}</h1></div>
        <div><span class="subheader">{{subtags}}</span></div>
        <div class="arrow-container"><div class="arrow-right"></div></div>
        <div class="rowcolor"><span class="tcat">Roll: </span>{{roll}}</div>
    </div>
</rolltemplate>

<script type="text/worker">
const stats = ['str','dex','con','int','wis','cha'];
stats.forEach(stat => {
    on(`change:${stat} sheet:opened`, () => {
        getAttrs([stat], values => {
            const stat_score = parseInt(values[stat], 10)||0;
            const stat_modifier = Math.floor(stat_score/2) -5;
            setAttrs({
                [`${stat}_mod`]: stat_modifier 
            });
        });
    });
});
</script>
