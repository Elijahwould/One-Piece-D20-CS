<rolltemplate class="sheet-rolltemplate-5eAttack">
    <div class="container">
        <div><h1>{{name}}</h1></div>
        <div><span class="subheader">{{subtags}}</span></div>
        <div class="arrow-container"><div class="arrow-right"></div></div>
        <div class="rowcolor"><span class="tcat">Attack: </span>{{attack}}</div>
        {{#damage}}
            <div>
                 <span class="tcat">Damage: </span>{{damage}}

            </div>
        {{/damage}}
        {{#rollWasCrit() attack}}
            <div>
                    <span class="tcat"></span>{{dmgcrit}}
            </div>
        {{/rollWasCrit() attack}}
        {{#snkattk}}
            <div>
                 <span class="tcat">Sneak Attack: </span>{{snkattk}} 
                 <span class="tcat">Crit: </span>{{snkcrit}}
            </div>
        {{/snkattk}}
    </div>
</rolltemplate>

<rolltemplate class="sheet-rolltemplate-PSkillCheck">
	<div class="container">
		<div><h1>{{name}}<h1></div>
		<div><span class="subheader">{{subtags}}</span></div>
		<div class="arrow-container"><div class="arrow-right"></div></div>
